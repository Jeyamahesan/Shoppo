
platform :ios, '9.0'

# ignore all warnings from all pods
inhibit_all_warnings!

#shared common pods
def import_pods_common
    
    pod 'AFNetworking', '3.1.0'

    pod 'SDWebImage', '~>3.7'

    pod 'Moltin', :git => 'https://github.com/moltin/ios-sdk.git', :branch => 'master'

end


#iPhone
target 'Shoppo' do

    import_pods_common

end
